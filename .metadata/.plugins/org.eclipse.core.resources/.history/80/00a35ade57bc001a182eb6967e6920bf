package vn.baitap.java;

public class Giaithua {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x;
		System.out.println("Giai thua cua " + x + " la:" + Giaithua.tinhGiaithua(a)); 
	}
		
		 public static long tinhGiaithua(int n) {
		        long giai_thua = 1;
		        if (n == 0 || n == 1) {
		            return giai_thua;
		        } else {
		            for (int i = 2; i <= n; i++) {
		                giai_thua *= i;
		            }
		            return giai_thua;
		        }

	}

}
